{
  "name": "obvy-imggen",
  "version": "1.0.0",
  "description": "AWS Image Generation Platform - Production-ready image generation with SageMaker",
  "private": true,
  "workspaces": [
    "services/*",
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "npm install && npm run build:shared",
    "build": "npm run build:shared && npm run build:services && npm run build:apps",
    "build:shared": "npm run build --workspace=packages/shared",
    "build:services": "npm run build --workspace=services/api && npm run build --workspace=services/worker",
    "build:apps": "npm run build --workspace=apps/web",
    "dev": "npm run dev --workspace=apps/web",
    "lint": "npm run lint --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "test:integration": "npm run test:integration --workspaces --if-present",
    "test:e2e": "npm run test:e2e --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "prettier": "^3.2.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/obvy-imggen.git"
  },
  "keywords": [
    "aws",
    "sagemaker",
    "image-generation",
    "stable-diffusion",
    "terraform",
    "serverless"
  ],
  "author": "Your Organization",
  "license": "MIT"
}